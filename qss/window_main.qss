QWidget#window_buttons{
    border-top-left-radius:5px;
    border-top-right-radius:5px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgb(202,12,22),stop:1 rgb(255,102,51));
}

QPushButton#button_min_window{
    background:transparent;
    border-image:url(images/minimize-window.png);
}

QPushButton#button_min_window:hover{
    background-color:rgb(255, 0, 0);
}

QPushButton#button_close_window{
    background:transparent;
    border-image:url(images/close-window.png);
}

QPushButton#button_close_window:hover{
    background-color:rgb(255, 0, 0);
}

QWidget#head{
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgb(202,12,22),stop:1 rgb(255,102,51));
}

QWidget#body{
    background-color:white;
}

QLabel#user_photo{
    background-color:white;
    border-radius:40px;
}

QLabel#user_name{
    font-size:24px;
    font-family:Arial,"微软雅黑";
    font-weight:bold;
    margin-left:10px;
    padding:0px;
}

QLabel#user_info{
    font-size:18px;
    font-family:Arial,"微软雅黑";
    margin-left:10px;
    padding:0px;
}

QComboBox#env{
    font-size:14px;
    font-family:"微软雅黑";
    height:30px;
    padding-left:5px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:1,stop:0 rgb(255,51,51),stop:1 rgb(255,153,102));
    border:0px;
}

QLabel#search_icon{
    background:rgb(255,153,102);
    background-image:url(images/search.png);
    background-position:center;
    background-repeat:none;
}

QLineEdit#search_tenant{
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgb(255,153,102),stop:1 rgb(255,255,255));
    border:0px;
    font-family:"微软雅黑";
    font-size:14px;
}

QPushButton#fold_button{
    border:0px;
    background-image:url(images/arrow_down.png);
    background-position:center;
    background-repeat:none;
}

QLabel#group_name{
    padding-left:10px;
    font-size:14px;
    font-family:"微软雅黑";
    font-weight:bold;
}

QLabel#tenant{
    margin-bottom:10px;
    padding-left:40px;
    font-family:"微软雅黑";
    font-size:14px;
}

QWidget#bottom{
    border-bottom-left-radius:5px;
    border-bottom-right-radius:5px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgb(202,12,22),stop:1 rgb(255,102,51));
}

QPushButton#settings{
    background:transparent;
    padding:1px 10px 1px 10px;
    font-size:14px;
    font-family:"微软雅黑";
    height:45px;
}

QPushButton#settings:hover{
    background-color:rgb(255,102,51);
    border-bottom-left-radius:5px;
}

QListView#env_list::item{
    height:30px;
    font-size:14px;
    font-family:"微软雅黑";
    padding-left:5px;
    background:qlineargradient(spread:pad,x1:0,y1:0,x2:1,y2:0,stop:0 rgb(255,153,102),stop:1 rgb(255,255,255));
}

QPushButton#tab{
    background-color:white;
    font-size:16px;
    font-family:"微软雅黑";
    border:None;
    text-align:justify;
}

QPushButton#tab:checked{
    border:None;
    border-bottom:2px solid red;
}